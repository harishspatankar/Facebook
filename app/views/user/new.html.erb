<%= form_with scope: :user, url: user_index_url, local: true do |form| %>
<h1>Register</h1>
<table>
  <tr>
    <td> <%= form.label :Name %> </td>
    <td> <%= form.text_field :name %> </td>
  </tr>
  <tr>
    <td> <%= form.label :Email %> </td>
    <td> <%= form.text_field :email %> </td>
  </tr>
  <tr>
    <td> <%= form.label :Gender %> </td>
    <td><%= form.text_field :gender %></td>
  </tr>
  <tr>
    <td> <%= form.label :Date_of_birth %> </td>
    <td> <%= form.date_field :date_of_birth %> </td>
  </tr>
  <tr>
    <td> <%= form.label :Password %> </td>
    <td> <%= form.password_field :password %> </td>
  </tr>
  <tr>
    <td> <%= form.label :Confirm_Password %> </td>
    <td> <%= form.password_field :password_confirmation %> </td>
  </tr>
</table>
    <%= form.submit %>
    <%= form.button :Reset%>
  </p>
<% end %>
